@model IEnumerable<RentalService.Models.User>
@{
    ViewData["Title"] = "Quản lý người dùng";
}
<h2>Quản lý người dùng</h2>
<table class="table">
    <thead>
        <tr>
            <th>Họ tên</th>
            <th>Email</th>
            <th>Vai trò</th>
        </tr>
    </thead>
    <tbody>
    @foreach (var user in Model)
    {
        <tr>
            <td>@user.Name</td>
            <td>@user.Email</td>
            <td>@user.Role</td>
        </tr>
    }
    </tbody>
</table>
